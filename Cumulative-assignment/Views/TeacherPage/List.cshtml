@model IEnumerable<Cumulative_assignment.Models.Teacher>

<a style="margin-bottom:10px;" href="/TeacherPage/New">Add Teacher</a>
<form method="GET" action="/TeacherPage/List">
    <div>
        <input type="text" name="SearchKey"/>
        <input type="submit" value="Search" />
    </div>
</form>

@foreach (Cumulative_assignment.Models.Teacher Teacher in Model)
{
    <div>
        <a href="/TeacherPage/Show/@Teacher.TeacherId">@Teacher.TeacherFName @Teacher.TeacherLName</a>
    </div>
}