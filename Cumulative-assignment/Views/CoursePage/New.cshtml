<h1>Add a new Course</h1>

<form method="POST" action="/CoursePage/Create">
    <div>
        <label for="CourseCode">Course Code:</label>
        <input type="text" name="CourseCode" id="CourseCode" />
    </div>
    <div>
        <label for="CourseTeacher">Teacher for the Course:</label>
        <input type="text" name="CourseTeacher" id="CourseTeacher" />
    </div>
    <div>
        <label for="CourseStartDate">Start date</label>
        <input type="text" name="CourseStartDate" id="CourseStartDate" />
    </div>
    <div>
        <label for="CourseFinishDate">Finish date</label>
        <input type="text" name="CourseFinishDate" id="CourseFinishDate" />
    </div>
    <div>
        <label for="CourseName">Course Name:</label>
        <input type="text" name="CourseName" id="CourseName" />
    </div>
    <div>
        <input type="submit" value="Add Article"/>
    </div>
</form>