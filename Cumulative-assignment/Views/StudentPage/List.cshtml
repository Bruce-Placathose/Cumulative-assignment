@model IEnumerable<Cumulative_assignment.Models.Student>

<a style="margin-bottom:10px;" href="/StudentPage/New">Add Student</a>
<form method="GET" action="/StudentPage/List">
    <div>
        <input type="text" name="SearchKey"/>
        <input type="submit" value="Search" />
    </div>
</form>

@foreach (Cumulative_assignment.Models.Student Student in Model)
{
    <div>
        <a href="/StudentPage/Show/@Student.StudentId">@Student.StudentFName @Student.StudentLName</a>
    </div>
}